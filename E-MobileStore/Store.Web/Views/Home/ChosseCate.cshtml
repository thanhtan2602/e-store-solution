@model IEnumerable<Store.WebService.ViewModels.vmCategory>;
@{
    ViewData["Title"] = "ChosseCate";
}

<ul class="choose-cate  ">
    @if(Model != null)
    {
        @foreach(var category in Model)
        {
            <li>
                <a href="@Url.Action("Index", "Category", new { categoryUrl = category.CategoryUrl })">
                    <div class="img-catesp cateiphone">
                        <img class="lazyload" data-src="@category.ImageURL" alt="@category.Name" width="102" height="112">
                    </div>
                    <span>@category.Name</span>
                </a>
            </li>
        }
    }
</ul>
